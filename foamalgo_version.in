#include <string>


inline std::string getFoamalgoVersion()
{
  return "@foamalgo_VERSION@";
}

inline unsigned getFoamalgoVersionMajor()
{
  return @foamalgo_VERSION_MAJOR@;
}

inline unsigned getFoamalgoVersionMinor()
{
  return @foamalgo_VERSION_MINOR@ +0;
}

inline unsigned getFoamalgoVersionPatch()
{
  return @foamalgo_VERSION_PATCH@ +0;
}

inline unsigned getFoamalgoVersionTweak()
{

 return @foamalgo_VERSION_TWEAK@ +0;
}
